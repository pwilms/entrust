{
    "name": "parsidev/entrust",
    "description": "This package provides a flexible way to add Role-based Permissions to Laravel 5.1",
    "keywords": ["laravel","illuminate","auth","roles","acl","permission"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Reza Parsaeian",
            "email": "info@parsidev.ir"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/console": "5.1.x-dev",
        "illuminate/support": "5.1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "mockery/mockery": "dev-master",
        "illuminate/database": "5.1.x-dev",
        "sami/sami": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/commands"
        ],
        "psr-4": {
            "Parsidev\\Entrust\\": "src/Entrust/"
        }
    },
    "minimum-stability": "dev"
}
